{
  "private": true,
  "name": "gotchiminer-server",
  "version": "1.0.0",
  "description": "npm init template for bootstrapping an empty Colyseus project",
  "main": "build/index.js",
  "scripts": {
    "version": "tsc -v",
    "start": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start:prod": "node build/src/index.js",
    "loadtest": "colyseus-loadtest loadtest/Classic.ts --room 6_Classic --numClients 32",
    "build": "npm run clean && tsc && node node_modules/copyfiles/copyfiles package.json ./build && node node_modules/copyfiles/copyfiles arena.env ./build",
    "clean": "node node_modules/rimraf/bin build",
    "test": "mocha --require ts-node/register test/**_test.ts --exit --timeout 15000"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/colyseus/create-colyseus/issues"
  },
  "homepage": "https://github.com/colyseus/create-colyseus#readme",
  "devDependencies": {
    "@colyseus/loadtest": "^0.14.4",
    "@colyseus/testing": "^0.14.21",
    "@types/chroma-js": "^2.1.3",
    "@types/express": "^4.17.1",
    "@types/jsdom": "^16.2.13",
    "@types/mocha": "^8.2.3",
    "@types/lodash": "^4.14.182",
    "@types/sqlite3": "^3.1.8",
    "@types/tmp": "^0.2.3",
    "@types/simple-peer": "^9.11.4",
    "@types/uuid": "^8.3.4",
    "copyfiles": "^2.4.1",
    "express": "^4.17.3",
    "mocha": "^9.2.2",
    "rimraf": "^2.7.1",
    "ts-node": "^8.1.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@colyseus/arena": "^0.14.22",
    "@colyseus/monitor": "^0.14.20",
    "@colyseus/schema": "^2.0.3",
    "@colyseus/uwebsockets-transport": "^0.14.27",
    "canvas": "^2.10.2",
    "chisel-api-interface": "^1.24.0",
    "chroma-js": "^2.4.2",
    "colyseus": "^0.14.24",
    "generate-password-ts": "^1.6.3",
    "global-jsdom": "8.4.0",
    "gotchiminer-multiplayer-protocol": "^1.14.0",
    "jsdom": "20.0.2",
    "lodash": "^4.17.21",
    "mathjs": "^10.0.2",
    "phaser": "^3.55.2",
    "sqlite3": "^5.1.2",
    "tmp": "^0.2.1",
    "uuid": "^8.3.2",
    "uwebsockets-express": "^1.2.2",
    "web3": "^1.7.5"
  },
  "overrides": {
    "@colyseus/uwebsockets-transport": {
      "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.14.0"
    }
  }
}
